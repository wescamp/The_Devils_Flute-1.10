msgstr ""

#. [message]: speaker=Bruggwy
#: ./The_Devils_Flute/scenarios/02_Anyguests_Inn.cfg:790
msgid "Arghh..."
msgstr ""

#. [message]: speaker=Creona
#: ./The_Devils_Flute/scenarios/02_Anyguests_Inn.cfg:794
